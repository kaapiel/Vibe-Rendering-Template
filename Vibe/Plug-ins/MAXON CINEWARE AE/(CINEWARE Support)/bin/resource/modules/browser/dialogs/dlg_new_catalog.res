// C4D-DialogResourceDIALOG DLG_NEW_CATALOG{  NAME TITLE_STR_NEW_CATALOG;	SCALE_H; SCALE_V;  	GROUP	{		COLUMNS 2; 		BORDERSIZE 8,8,8,8;		FIT_H;				STATICTEXT { ALIGN_LEFT; NAME STR_NAME; }		EDITTEXT GADGET_CATALOG_NAME { SCALE_H; SCALE_V; SIZE 256, 0; };    STATICTEXT { NAME STR_BASE_FOLDER; CENTER_V; ALIGN_LEFT; }    GROUP    {      COLUMNS 2;      SPACE 4, 4;            EDITTEXT GADGET_CATALOG_BASE_FOLDER { CENTER_V; FIT_H; SCALE_H; SIZE 200, 0; }      BUTTON GADGET_CATALOG_BROWSE { NAME STR_CATALOG_BROWSE; ALIGN_TOP; ALIGN_LEFT; }    }	}		DLGGROUP { OK; CANCEL; }}